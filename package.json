{
  "name": "voice-slot-scheduler",
  "version": "0.1.0",
  "private": true,
  "description": "Queue and dispatch calls to ElevenLabs respecting active concurrent slot limits.",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
